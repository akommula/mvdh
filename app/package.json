{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "export GOOGLE_APPLICATION_CREDENTIALS=$(cat client_secret.json) && gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "export GOOGLE_APPLICATION_CREDENTIALS=$(cat client_secret.json) && npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy-test": "surge public/ https://mvdh-test.surge.sh",
    "deploy-prod": "surge public/ mvdh.club"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "browserslist": "^4.14.0",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-build-date": "^1.0.0",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-source-google-spreadsheets": "^3.0.5",
    "gatsby-transformer-sharp": "^2.5.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "tailwindcss": "^1.7.2"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ]
}
